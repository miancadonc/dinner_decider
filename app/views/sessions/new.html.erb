<h1>Log in</h1>

<%=  form_with url:"/login", method: :post do |f| %>

<%= f.label :email %>
<%= f.email_field :email %>
<br>
<%= f.label :password %>
<%= f.password_field :password %>
<br>
<%= f.submit "Log In" %>
<% end %>

<h3>Or <%= button_to "Log in with Github", '/auth/github', method: 'post' %> </h3>